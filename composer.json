{
    "name": "park-manager/hub-kit",
    "description": "HubKit allows project(s) maintainers to easily manage their GitHub repositories",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/process": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/console": "^6.0",
        "webmozart/console": "^2.0",
        "pimple/pimple": "^3.0",
        "guzzlehttp/psr7": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "knplabs/github-api": "^3.0",
        "ddd/slug": "~1.0.0",
        "rollerworks/version": "^0.3.1",
        "composer/semver": "^3.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.21",
        "phpspec/prophecy-phpunit": "^v2.0.1",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/error-handler": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "HubKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HubKit\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-1.x": "1.2-dev"
        }
    },
    "repositories": [
        {
            "url": "https://github.com/park-manager/webmozart-console.git",
            "type": "github"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
