{
    "name": "park-manager/hub-kit",
    "description": "HubKit allows project(s) maintainers to easily manage their GitHub repositories",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "symfony/process": "~3.4",
        "symfony/event-dispatcher": "~3.4",
        "symfony/filesystem": "~3.4",
        "symfony/console": "^3.4",
        "webmozart/console": "dev-master@dev",
        "pimple/pimple": "^3.0",
        "guzzlehttp/psr7": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "knplabs/github-api": "^3.0",
        "ddd/slug": "~1.0.0",
        "rollerworks/version": "^0.3.1",
        "composer/semver": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3.1|^8.5.12",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/debug": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "HubKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HubKit\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-1.x": "1.0-dev"
        }
    }
}
